		<rdfs:label xml:lang="en">speed limit 30</rdfs:label>
		<skos:definition xml:lang="en">speed limit of 30 km per hour</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/AV/VehicleAutomationLevels_2018/SpeedLimit40">
		<rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/AV/VehicleAutomationLevels_2018/SpeedLimit"/>
		<rdfs:label xml:lang="en">speed limit 40</rdfs:label>
		<skos:definition xml:lang="en">speed limit of 40 km per hour</skos:definition>
