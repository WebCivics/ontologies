	<owl:Class rdf:about="&fibo-fbc-fct-ireg;BusinessIdentifierCodeDataRecord">
		<rdfs:subClassOf rdf:resource="&fibo-fbc-fct-breg;BusinessRegistryEntry"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isIncludedIn"/>
				<owl:hasValue rdf:resource="&fibo-fbc-fct-ireg;BusinessIdentifierCodeRegistry"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fbc-fct-breg;isSelfMaintained"/>
