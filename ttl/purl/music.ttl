<http://purl.org/ontology/mo/Activity>
  a owl:Class ;
  ns1:level "1" ;
  rdfs:comment """
        An activity period, defining when an artist was musically active.
    """ ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "activity" ;
  rdfs:subClassOf <http://purl.org/NET/c4dm/event.owl#Event> ;
  ns2:term_status "testing" .

ns1:AnalogSignal
  a owl:Class ;
  ns1:level "2" ;
  rdfs:comment """
        An analog signal.
    """ ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "analogue signal" ;
  rdfs:subClassOf ns1:Signal ;
  owl:disjointWith ns1:DigitalSignal ;
  ns2:term_status "stable" .

ns1:Arrangement
  a owl:Class ;
  ns1:level "2" ;
  rdfs:comment """
        An arrangement event.
        Takes as agent the arranger, and produces a score (informational object, not the actually published score).
    """ ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "arrangement" ;
  rdfs:subClassOf <http://purl.org/NET/c4dm/event.owl#Event> ;
  ns2:term_status "stable" .

ns1:Arranger
  a owl:Class ;
  ns1:level "2" ;
  rdfs:label "arranger" ;
  rdfs:subClassOf foaf:Agent ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/NET/c4dm/event.owl#isAgentIn> ;
    owl:someValuesFrom ns1:Arrangement
  ] ;
  ns2:term_status "unstable" .

ns1:AudioFile
  a owl:Class ;
  ns1:level "1" ;
  rdfs:comment "An audio file, which may be available on a local file system or through http, ftp, etc." ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "audio file" ;
  rdfs:subClassOf ns1:Medium, foaf:Document ;
  ns2:term_status "unstable" .

ns1:CD
  a owl:Class ;
  ns1:level "1" ;
  rdfs:comment "Compact Disc used as medium to record a musical manifestation." ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "CD" ;
  rdfs:subClassOf ns1:Medium ;
  ns2:term_status "unstable" .

ns1:Composer
  a owl:Class ;
  ns1:level "2" ;
  rdfs:label "composer" ;
  rdfs:subClassOf foaf:Agent ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty <http://purl.org/NET/c4dm/event.owl#isAgentIn> ;
    owl:someValuesFrom ns1:Composition
  ] ;
  ns2:term_status "unstable" .

ns1:Composition
  a owl:Class ;
  ns1:level "2" ;
  rdfs:comment """
        A composition event.
        Takes as agent the composer himself.
        It produces a MusicalWork, or a MusicalExpression (when the initial "product" is a score, for example), or both...
    """ ;
  rdfs:isDefinedBy ns1: ;
  rdfs:label "composition" ;
  rdfs:subClassOf <http://purl.org/NET/c4dm/event.owl#Event> ;
  ns2:term_status "stable" .

ns1:Conductor
  a owl:Class ;
  ns1:level "2" ;
  rdfs:label "conductor" ;
  rdfs:subClassOf foaf:Agent ;
