<http://purl.org/ontology/mo/>
  a owl:Ontology ;
  dc11:created "2006/12/21 12:00:00" ;
  dc11:date "2013/07/22 16:54:19" ;
  dc11:description """
        The Music Ontology Specification provides main concepts and 
        properties fo describing music (i.e. artists, albums and tracks) 
        on the Semantic Web. 
    """ ;
  dc11:title "The Music Ontology" ;
  ns0:preferredNamespacePrefix "mo" ;
  ns0:preferredNamespaceUri "http://purl.org/ontology/mo/" ;
  owl:imports <http://purl.org/NET/c4dm/event.owl>, <http://purl.org/NET/c4dm/keys.owl>, dc:, <http://purl.org/ontology/ao/core>, <http://purl.org/vocab/bio/0.1/>, <http://purl.org/vocab/frbr/core>, <http://www.w3.org/2006/time>, foaf: ;
